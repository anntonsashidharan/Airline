<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
           version="2.5">

    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.airline.controller.login.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MainController</servlet-name>
        <servlet-class>com.airline.controller.main.MainController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdatePasswordController</servlet-name>
        <servlet-class>com.airline.controller.updatePassword.UpdatePasswordController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutConroller</servlet-name>
        <servlet-class>com.airline.controller.login.LogoutConroller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet</servlet-name>
        <servlet-class>com.airline.controller.login.Servlet</servlet-class>
    </servlet>
</web-app>
